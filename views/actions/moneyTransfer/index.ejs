<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">転送アクション検索</h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="/">Home</a>
                    </li>
                    <li class="breadcrumb-item active">転送アクション検索</li>
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-primary">
                    <!-- <div class="card-header">
                        <h3 class="card-title">Quick Example</h3>
                    </div> -->
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form role="form" method="get">
                        <div class="card-body">
                            <div class="form-group">
                                <label for="accountNumber">口座タイプ</label>
                                <input type="text" class="form-control" id="accountType" name="accountType"
                                    placeholder="口座タイプ" value="<%= query.accountType %>">
                            </div>
                            <div class="form-group">
                                <label for="accountNumber">口座番号</label>
                                <input type="text" class="form-control" id="accountNumber" name="accountNumber"
                                    placeholder="口座番号" value="<%= query.accountNumber %>">
                            </div>
                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary">検索</button>
                        </div>
                    </form>
                </div>
                <!-- /.card -->

            </div>
            <!--/.col (left) -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">検索結果</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body table-responsive">
                        <table id="actions-table" class="table table-bordered table-hover dataTable">
                            <thead>
                                <tr>
                                    <th>Action Type<br>Start Date<br>End Date<br>Status</th>
                                    <th>From</th>
                                    <th>To</th>
                                    <th>Amount<br>Description</th>
                                    <th>Transaction</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>Action Type<br>Start Date<br>End Date<br>Status</th>
                                    <th>From</th>
                                    <th>To</th>
                                    <th>Amount<br>Description</th>
                                    <th>Transaction</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>
<!-- /.content -->

<!-- DataTables -->
<link rel="stylesheet" href="/node_modules/admin-lte/plugins/datatables/dataTables.bootstrap4.css">
<script src="/js/actions/moneyTransfer/index.js"></script>